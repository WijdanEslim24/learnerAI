# Environment Variables for LearnerAI Frontend

# API Configuration
VITE_API_URL=http://localhost:5000

# Supabase Configuration (if needed for direct frontend access)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
VITE_APP_NAME=LearnerAI
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_FEATURES=true

# External Service URLs
VITE_SKILLS_ENGINE_URL=http://localhost:5001
VITE_COURSE_BUILDER_URL=http://localhost:5002
