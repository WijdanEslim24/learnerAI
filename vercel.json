{
  "version": 2,
  "name": "learner-ai-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "@api_url"
  },
  "functions": {
    "frontend/dist/**/*.js": {
      "maxDuration": 10
    }
  }
}
